            <i-grid-icon>
                <image src="../images/icon-vip.png" />
            </i-grid-icon>
            <i-grid-label>书评</i-grid-label>
        </i-grid-item>
        <i-grid-item>
            <i-grid-icon>
                <image src="../images/icon-vip.png" />
