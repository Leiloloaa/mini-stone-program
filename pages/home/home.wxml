        <view class="container">
            <view class="btns">
                <view class="btn">
                    打卡
                    <i-count-down target="{{targetTime}}" countdown-class="big" clear-timer="{{clearTimer}}"></i-count-down>
                </view>
            </view>
        </view>
        <view class="padding">
            <view class="cu-avatar-group">
                <view class="cu-avatar round" wx:for="{{4}}" wx:key style="background-image:url(https://ossweb-img.qq.com/images/lol/web201310/skin/big1000{{index+1}}.jpg);"></view>
            </view>
            今日已完成打卡 38 人
        </view>
    </view>
    <view class="space"></view>
    <view class="space"></view>
    <i-grid>
        <i-grid-item>
