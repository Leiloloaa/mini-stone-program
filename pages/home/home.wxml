        </i-grid-item>
        <i-grid-item>
            <i-grid-icon>
                <image src="../../images/angular.svg" />
            </i-grid-icon>
            <i-grid-label>Angular</i-grid-label>
        </i-grid-item>
        <i-grid-item>
            <i-grid-icon>
                <image src="../images/icon-vip.png" />
            </i-grid-icon>
            <i-grid-label>React</i-grid-label>
        </i-grid-item>
    </i-grid>
    <i-grid>
